convert_to_continuous <- function(x){
x_min <- (x * 100 - as.integer(x * 100)) * 100
x_sec <- (x - as.integer(x) - x_min / 10000) * 100
return(as.integer(x) + x_sec / 60 + x_min / 60 / 60)
}
